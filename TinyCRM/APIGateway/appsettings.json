{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "tinycrm.people.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/people/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "SwaggerKey": "people"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "tinycrm.sales.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/sales/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "SwaggerKey": "sales"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "tinycrm.iam.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/iam/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "SwaggerKey": "iam"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "people",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "People.API",
          "Version": "1.0",
          "Url": "http://tinycrm.people.api:80/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "sales",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Sales.API",
          "Version": "1.0",
          "Url": "http://tinycrm.sales.api:80/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "iam",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "IAM.API",
          "Version": "1.0",
          "Url": "http://tinycrm.iam.api:80/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
